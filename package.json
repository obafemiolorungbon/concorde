{
  "name": "tailwindpurge",
  "version": "1.2.27",
  "description": "a minimal cli tool to help purge tailwind css without the tailwin set up",
  "main": "index",
  "bin": {
    "concorde": "bin/cli.js"
  },
  "scripts": {
    "checkin": "dir",
    "build": "postcss ./styles/config/tailwindbase.css -c ./tailwind.config.cjs -o ../../styles/output/tailwinddevcss.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "devBuild": "yarn run checkin && yarn run build",
    "miniBuild": "postcss ./styles/config/tailwindbase.css -c ./tailwind.config.cjs -o ../../styles/output/tailwindminicss.css",
    "compile": "gulp compile",
    "commit": "git add . && git commit -m 'fix: fix errors' && git push -u origin master",
    "lauch": "yarn run setup && yarn run compile && yarn run build"
  },
  "keywords": [
    "purge",
    "tailwind",
    "cli",
    "open",
    "source"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-inject-string": "^1.1.2",
    "gulp-rename": "^2.0.0",
    "inquirer": "^8.0.0",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "shelljs": "^0.8.4",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "gulp-babel": "^8.0.0"
  }
}
